{
    "plugins": [
    ],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "quotes": [
            0,
            "single"
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "brace-style": [
            2,
            "1tbs",
            {
                "allowSingleLine": true
            }
        ],
        "func-style": [
            2,
            "declaration"
        ],
        "no-undef": "error",
        "space-before-function-paren": [
            0
        ],
        "keyword-spacing": 2,
        "max-len": [
            2,
            140,
            4
        ],
        "dot-notation": [
            2,
            {
                "allowPattern": "^[a-z]+(_[a-z]+)+$"
            }
        ],
        "eqeqeq": [
            2,
            "smart"
        ],
        "dot-location": [
            2,
            "property"
        ],
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-extend-native": [
            2
        ],
        "no-floating-decimal": 2,
        "no-magic-numbers": [
            2,
            {
                "ignore": [
                    -1,
                    0,
                    1,
                    2,
                    3
                ]
            }
        ],
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-new": 2,
        "vars-on-top": 0,
        "wrap-iife": [
            2,
            "outside"
        ],
        "strict": [
            0,
            "global"
        ],
        "no-lonely-if": 2,
        "no-multiple-empty-lines": [
            2,
            {
                "max": 2,
                "maxEOF": 1
            }
        ],
        "no-nested-ternary": 2,
        "yoda": 2,
        "quote-props": [
            2,
            "as-needed"
        ],
        "operator-linebreak": [
            2,
            "before"
        ]
    },
    "env": {
        "node": true
    },
    "globals": {
    },
    "extends": "eslint:recommended"
}